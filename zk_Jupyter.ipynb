    "## function that takes in a string and returns a sha256 hash of it\n",
    "from Crypto.Hash import SHA256\n",
    "print('Type anything and get the sha256 back')\n",
    "a = input()\n",
    "h = SHA256.new()\n",
    "h.update(a.encode('utf-8'))\n",
    "print(h.hexdigest())"
   ]
  }
